public class Box {
	int width;
	int length;
	int height;
	
	// 생성자는 여러개 생성 가능, 파라미터 부분 주의; 파라미터 값이 같으면 안됨. - 타입, 개수, 순서
	public Box() {
		width = 1;
		length = 1;
		height = 1; 
		// 0으로 초기화 할 때, 0 값을 적어주지 않아도 기본값으로 이미 초기화되어있음.
	}
	
	// 생성자 => 클래스 이름 그대로.
	public Box(int w, int l, int h) {
		width = w;
		length = l;
		height = h;
	}
	
	public static void main(String[] args) {
		Box b3 = new Box();
		// System.out.println(b3.width);
		// System.out.println(b3.length);
		// System.out.println(b3.height);
		
		// Box b = new Box(50, 60, 70); // 원하는 필드값으로 초기화.
		// System.out.println(b.width);
		// System.out.println(b.length);
		// System.out.println(b.height);
		
		// 박스 인스턴스화. 생성자를 호출해서 가로 90 세로 100 높이 110
		// Box b2 = new Box(90, 100, 110);
		// System.out.println(b2.width);
		// System.out.println(b2.length);
		// System.out.println(b2.height);
	}
}