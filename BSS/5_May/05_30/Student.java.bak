// 국어 점수
// 영어 점수
// 수학 점수

// 생성자
// getter/setter

// 자기 평균 점수를 알려줄 수 있음.

public class Student {
	private String name = "아무이름이나써";
	private int kor = 80;
	private int eng = 95;
	private int math = 60;
	private int sum = kor + eng + math;
	private int avrg = sum / 3;

	public String getName() {
		return name;
	}
	public String setName(String name) {
		this.name = name;
		return name;
	}

	public int getKor() {
		return kor;
	}
	public int setKor(int kor) {
		this.kor = kor;
		return kor;
	}
	public int getEng() {
		return eng;
	}
	public int setEng(int eng) {
		this.eng = eng;
		return eng;
	}
	public int getMath() {
		return math;
	}
	public int setMath(int math) {
		this.math = math;
		return math;
	}
	
	public int getSum() {
		return sum;
	}
	public int setSum(int sum) {
		this.sum = sum;
		return sum;
	}
	public int getAvrg() {
		return avrg;
	}
	public int setAvrg(int avrg) {
		this.avrg = avrg;
		return avrg;
	}
	
	void printAll() {
		System.out.println(name);
		System.out.printf("국어 : %d\n영어 : %d\n수학 : %d\n총합 : %d\n평균 : %d\n", kor, eng, math, sum, avrg);
	}
}