public class Star3 {
	public static void main(String[] args) {
		for (int i = 1; i <= 5; i++) { // 줄 반복
			// System.out.print("줄반복");
			for (int j = 5; j >= i; j--) { // 공백 반복 줄어들기
				System.out.print("  ");
			}
			for (int k = 1; k <= i; k++) { // 별 반복 늘어나기
				System.out.print("*");
			}
			System.out.println();
		}
	}
}
