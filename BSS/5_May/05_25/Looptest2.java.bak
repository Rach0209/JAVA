// (1) (2) (3) (4) (5)
// (5) (4) (3) (2) (1)
// (1) (2) (3) (4) (5)
// (5) (4) (3) (2) (1)
// (1) (2) (3) (4) (5)

public class Looptest2 {
	public static void main(String[] args) {
		for(int i = 1; i < 6; i++) {
			if (i % 2 == 0) {
				for (int j = 1; j <=5; j++) {
					System.out.print("(" + j + ") ");
				}
				System.out.println();
			} else {
				for (int j = 5; j >= 1; j--) {
					System.out.print("(" + j + ") ");
				}
				System.out.println();
			}
		}
	}
}