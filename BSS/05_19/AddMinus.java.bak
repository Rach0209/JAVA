// 사용자에게 정수 하나를 입력받아서
// 입력 값 -2 ~ +2 범위의 정수를 출력해보세요
// 예) 7 -> 5 6 7 8 9
import java.util.Scanner;

public class AddMinus {
	public static void main(String[] args) {
		Scanner scan = new Scanner(System.in);
		
		System.out.print("정수 하나를 입력 해주세요 : ");
		int a = scan.nextInt();
		// int numberCopy = number; 
		
		--a;
		System.out.print(--a + " ");
		System.out.print(++a + " ");
		System.out.print(++a + " ");
		System.out.print(++a + " ");
		System.out.println(++a + " ");
		
		System.out.print("정수 하나를 입력 해주세요 : ");
		int b = scan.nextInt();
		
		System.out.print((b -= 2) + " ");
		System.out.print((b += 1) + " ");
		System.out.print((b += 1) + " ");
		System.out.print((b += 1) + " ");
		System.out.println((b += 1) + " ");
		
		System.out.print("정수 하나를 입력 해주세요 : ");
		int c = scan.nextInt();
		c -= 2;
		System.out.print(c + " ");
		c++;
		System.out.print(c + " ");
		c++;
		System.out.print(c + " ");
		c++;
		System.out.print(c + " ");
		c++;
		System.out.print(c + " ");
		
	}
}